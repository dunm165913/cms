{
  "name": "be",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "egg": "^2.14.2",
    "eslint-config-prettier": "^3.4.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.10.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.12.0",
    "eslint-config-egg": "^7.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "prettier-eslint-cli": "^4.7.1",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "dev": "egg-bin dev",
    "test": "eslint '**/*.js' && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "prettier-eslint --write '**/*.js'",
    "ci": "npm run lint && egg-bin pkgfiles --check && npm run cov",
    "autod": "autod",
    "pkgfiles": "egg-bin pkgfiles"
  },
  "ci": {
    "version": "8, 9"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "egg",
    "egg-framework"
  ],
  "author": "",
  "files": [
    "index.js",
    "lib",
    "app",
    "config",
    "prettier.config.js"
  ],
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write '**/*.js'",
      "git add ."
    ]
  }
}
